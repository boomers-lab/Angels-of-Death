<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<ThingDef Abstract="true" Name="GW_SM_HelmetBase" ParentName="ArmorHelmetMakeableBase">
		<techLevel>Spacer</techLevel>
		<description>Astartes helmets are robust and angular, designed for protection and communication. They feature a distinctive visor for enhanced vision, often with a grill-like mouthpiece. Integrated systems include targeting optics, respiratory filters, and vox-communicators. Each helmet can be customized with chapter-specific symbols and colors.</description>
		<graphicData>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>1.6</drawSize>
		</graphicData>
		<recipeMaker>
			<unfinishedThingDef>UnfinishedTechArmor</unfinishedThingDef>
			<skillRequirements>
				<Crafting>15</Crafting>
			</skillRequirements>
			<recipeUsers Inherit="False">
				<li>GW_SM_ArmorBench</li>
			</recipeUsers>
		</recipeMaker>
		<uiIconScale>1.0</uiIconScale>
		<tradeTags>
			<li>HPHelmet</li>
		</tradeTags>
		<apparel>
			<tags>
				<li>Spacer</li>
				<li>EVA</li>
			</tags>
			<drawData>
				<!--<scale>1.5</scale> Uncomment these to increase size while equipped
                <childScale>1.5</childScale>-->
			</drawData>
			<useWornGraphicMask>true</useWornGraphicMask>
			<countsAsClothingForNudity>false</countsAsClothingForNudity>
			<bodyPartGroups>
				<li>FullHead</li>
			</bodyPartGroups>
			<layers>
				<li>EyeCover</li>
				<li MayRequire="ceteam.combatextended">StrappedHead</li>
				<li MayRequire="ceteam.combatextended">OnHead</li>
				<li>Overhead</li> <!--Needs to be last for the tool to properly label this a helmet-->
			</layers>
			<defaultOutfitTags>
				<li>Soldier</li>
			</defaultOutfitTags>
		</apparel>
	</ThingDef>

	<!-- Scout-->
	<ThingDef Name="GW_SM_ScoutHelmet_Base" ParentName="GW_SM_HelmetBase">
		<defName>GW_SM_ScoutHelmet_A</defName>
		<label>scout helmet (a)</label>
		<description>A Space Marine Scout helmet, used alongside its armor. It is reasonably protective, but its light weight is its main appeal.</description>
		<graphicData>
			<texPath>Things/Apparel/Armor_Scouts/Display/Scout_helmetA</texPath>
		</graphicData>
		<uiIconPath>Things/Apparel/Armor_Scouts/Display/Scout_helmetA</uiIconPath>
		<apparel>
			<renderSkipFlags>
				<li>None</li>
			</renderSkipFlags>
			<tags Inherit="False">
				<!--<li>Spacer</li>-->
				<li>GW_Scout</li>
			</tags>
			<wornGraphicPath>Things/Apparel/Armor_Scouts/Head/Helmet/Scout_helmetA</wornGraphicPath>
		</apparel>
		<comps>
			<li Class="GW4KArmor.CompProperties_TriColorMask">
				<maskCount>2</maskCount>
				<palettePresets>GW_SM_HelmetChapterPalettes</palettePresets>
				<defaultPalette>
					<colorA>(250, 176, 67)</colorA>
					<colorB>(250, 176, 67)</colorB>
					<colorC>(180, 65, 74)</colorC>
				</defaultPalette>
			</li>
		</comps>
		<equippedStatOffsets>
			<AimingDelayFactor>-0.1</AimingDelayFactor>
		</equippedStatOffsets>
		<costList>
			<ComponentSpacer>1</ComponentSpacer>
			<Plasteel>50</Plasteel>
			<Steel>30</Steel>
			<Uranium>10</Uranium>
			<HP_Ceramite>5</HP_Ceramite>
		</costList>
		<statBases>
			<WorkToMake>15000</WorkToMake>
			<MaxHitPoints>400</MaxHitPoints>
			<Mass>1.5</Mass>
			<Flammability>0.2</Flammability>
			<ArmorRating_Sharp>1</ArmorRating_Sharp>
			<ArmorRating_Blunt>0.65</ArmorRating_Blunt>
			<ArmorRating_Heat>0.7</ArmorRating_Heat>
			<Insulation_Cold>10</Insulation_Cold>
			<Insulation_Heat>10</Insulation_Heat>
			<EquipDelay>2</EquipDelay>
		</statBases>
		<recipeMaker>
			<researchPrerequisite>GW_SM_AstartesFabrication</researchPrerequisite>
		</recipeMaker>
	</ThingDef>

